# usually <=32768 (2^15), see /proc/sys/kernel/pid_max
@{pid}=[0-9]*

# same pattern as @{pid} for now
@{tid}=@{pid}

# synology build number
@{buildnum}=[0-9][0-9][0-9][0-9]

# vim:ft=apparmor
